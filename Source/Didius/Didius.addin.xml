<Addin id          = "Didius"              
       author      = "QNAX ApS"
       copyright   = "QNAX ApS"
       url         = "http://www.qnax.net"
       description = "Auction system"
       version     = "1.0">
	<Runtime>
		<Import assembly="Didius.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Sorento" version="1.0"/>
		<Addin id="sConsole" version="2.0"/>
	</Dependencies>
		
	<Extension path = "/SorentoLib/Init">
		<Type type="Didius.Addin.Init"/>
	</Extension>			
								
	<Extension path = "/SorentoLib/Resolver/Renders">
		<Type type="Didius.Addin.Render"/>
	</Extension>	
														
	<Extension path = "/SorentoLib/Resolver/Ajaxes">
		<Type type="Didius.Addin.Ajax"/>
	</Extension>				
	
	<Extension path = "/SorentoLib/Resolver/Functions">
		<Type type="Didius.Addin.Function"/>
	</Extension>	
	
	<Extension path = "/SorentoLib/Runtime">
		<Type type="Didius.Addin.Runtime"/>
	</Extension>
</Addin>