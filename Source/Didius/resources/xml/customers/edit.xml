<sui elementheight="40px">
	<layoutbox type="horizontal">			
		<panel tag="content" size="*">
			<tabview>
				<tab label="Detaljer" selected="true">
					<layoutbox type="horizontal" height="25%">			
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Kunde nr."/>
								</panel>					
								<panel size="*">
									<textbox tag="no" width="100%" readOnly="true"/>
								</panel>		
								<panel size="100px">
									<label text="Oprets dato"/>
								</panel>					
								<panel size="*">
									<textbox tag="createdate" width="100%" />
								</panel>				
							</layoutbox>
						</panel>				
					</layoutbox>									
					<layoutbox type="horizontal" height="25%">			
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Navn"/>
								</panel>					
								<panel size="*">
									<textbox tag="name" width="100%" />
								</panel>					
							</layoutbox>
						</panel>
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Att"/>
								</panel>					
								<panel size="*">
									<textbox tag="att" width="100%" />
								</panel>					
							</layoutbox>
						</panel>
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Adresse"/>
								</panel>					
								<panel size="*">
									<textbox tag="address1" width="100%" />
								</panel>					
							</layoutbox>
						</panel>	
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
								</panel>					
								<panel size="*">
									<textbox tag="address2" width="100%" />
								</panel>					
							</layoutbox>
						</panel>												
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Post nr."/>
								</panel>					
								<panel size="100px">
									<textbox tag="postcode" width="100%" />
								</panel>					
								<panel size="100px">
									<label text="By"/>
								</panel>					
								<panel size="*">
									<textbox tag="city" width="100%" />
								</panel>					
							</layoutbox>
						</panel>	
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Land"/>
								</panel>					
								<panel size="*">
									<textbox tag="country" width="100%" />
								</panel>					
							</layoutbox>
						</panel>												
					</layoutbox>					
					<layoutbox type="horizontal" height="25%">			
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Telefon"/>
								</panel>					
								<panel size="*">
									<textbox tag="phone" width="100%" />
								</panel>					
							</layoutbox>
						</panel>										
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Mobil"/>
								</panel>					
								<panel size="*">
									<textbox tag="mobile" width="100%" />
								</panel>					
							</layoutbox>
						</panel>														
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Email"/>
								</panel>					
								<panel size="*">
									<textbox tag="email" width="100%" />
								</panel>					
							</layoutbox>
						</panel>														
					</layoutbox>		
					<layoutbox type="horizontal" height="25%">			
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Moms"/>
								</panel>					
								<panel size="*">
									<checkbox tag="vat" />
								</panel>				
								<panel size="100px">
									<label text="Moms nr."/>
								</panel>					
								<panel size="*">
									<textbox tag="vatno" width="100%" />
								</panel>					
							</layoutbox>
						</panel>
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Bank"/>
								</panel>					
								<panel size="*">
									<textbox tag="bankname" width="100%" />
								</panel>					
							</layoutbox>
						</panel>						
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Reg. nr."/>
								</panel>					
								<panel size="100px">
									<textbox tag="bankregistrationno" width="100%" />
								</panel>					
								<panel size="100px">
									<label text="Konto nr."/>
								</panel>					
								<panel size="*">
									<textbox tag="bankaccountno" width="100%" />
								</panel>					
							</layoutbox>
						</panel>																					
					</layoutbox>					
				</tab>
				<tab label="Sager">
					<layoutbox type="horizontal">					
						<panel size="*">													
							<layoutbox type="vertical">
								<panel size="*">
									<listview tag="cases" width="100%" height="100%">
										<column tag="id" />
										<column tag="no" label="Sags nr." width="100px" visible="true" />
										<column tag="title" label="" width="200px" visible="true" />
									</listview>
								</panel>					
								<panel size="80px">
									<layoutbox type="horizontal" width="100%" height="100%">
										<panel size="*">
											<button tag="caseCreate" width="100%" label="Opret" />
											<button tag="caseEdit" width="100%" label="Redigere" disabled="true" />
											<button tag="caseDestroy" width="100%" label="Slet" disabled="true" />
										</panel>										
									</layoutbox>																									
								</panel>
							</layoutbox>														
						</panel>
					</layoutbox>			
				</tab>
				<tab label="Notat">
					<textarea tag="notes" />
				</tab>		
				<tab label="Grupper">
				</tab>		
				<tab label="Web">
				</tab>										
				<tab label="Log">
				</tab>						
			</tabview>		
		</panel>
	
		<panel tag="buttons" size="%elementheight%">			
			<layoutbox type="vertical">
				<panel size="*"/>
				<panel size="300px">
					<button tag="close" width="50%" label="Luk" />
					<button tag="save" width="50%" stylesheet="button green-gradient" label="Gem" disabled="true" />					
				</panel>				
			</layoutbox>
		</panel>	
	</layoutbox>
</sui>



