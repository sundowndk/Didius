<sui elementheight="40px">
	<layoutbox type="horizontal">			
		<panel tag="content" size="*">
			<tabview>
				<tab label="Detaljer" selected="true">
					<layoutbox type="horizontal">			
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Effekt nr."/>
								</panel>					
								<panel size="*">
									<textbox tag="no" width="100%" readOnly="true"/>
								</panel>		
								<panel size="100px">
									<label text="Oprets dato"/>
								</panel>					
								<panel size="*">
									<textbox tag="createdate" width="100%" />
								</panel>				
							</layoutbox>
						</panel>	
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Katalog nr."/>
								</panel>					
								<panel size="*">
									<textbox tag="catalogno" width="100%"/>
								</panel>		
							</layoutbox>
						</panel>		
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Title"/>
								</panel>					
								<panel size="*">
									<textbox tag="title" width="100%"/>
								</panel>		
							</layoutbox>
						</panel>		
						<panel size="%elementheight%">
							<layoutbox type="vertical">
								<panel size="100px">
									<label text="Beskrivelse"/>
								</panel>					
								<panel size="*">
									<textarea tag="description" width="100%" height="100px"/>
								</panel>		
							</layoutbox>
						</panel>		
					</layoutbox>									
				</tab>						
				<tab label="Bud">
					<layoutbox type="horizontal">			
						<panel size="*">
							<layoutbox type="vertical">
								<panel size="*">
									<listview tag="items" width="100%" height="100%">
										<column tag="id" />
										<column tag="amount" label="Bud" width="80px" visible="true" />
										<column tag="customer" label="Afgiver" width="200px" visible="true" />
										<column tag="createdate" label="Dato" width="150px" visible="true" />
									</listview>
								</panel>					
								<panel size="80px">
									<layoutbox type="horizontal" width="100%" height="100%">
										<panel size="*">
											<button tag="bidCreate" width="100%" label="Afgiv" disabled="true"/>
											<button tag="bidEdit" width="100%" label="Redigere" disabled="true" />
											<button tag="bidDestroy" width="100%" label="Slet" disabled="true" />
										</panel>										
									</layoutbox>																									
								</panel>
							</layoutbox>						
						</panel>
					</layoutbox>									
				</tab>						
			
			</tabview>		
		</panel>
	
		<panel tag="buttons" size="%elementheight%">			
			<layoutbox type="vertical">
				<panel size="*"/>
				<panel size="300px">
					<button tag="close" width="50%" label="Luk" />
					<button tag="save" width="50%" stylesheet="button green-gradient" label="Gem" disabled="true" />					
				</panel>				
			</layoutbox>
		</panel>	
	</layoutbox>
</sui>